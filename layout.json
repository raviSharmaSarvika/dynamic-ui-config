{
  "layout": [
    {
      "type": "ScrollView",
      "props": {
        "style": {
          "flex": 1,
          "padding": 16
        }
      },
      "children": [
        {
          "type": "View",
          "props": {
            "style": {
              "backgroundColor": "#f5f5f5",
              "padding": 20,
              "borderRadius": 10
            }
          },
          "children": [
            {
              "type": "Text",
              "props": {
                "children": "New Home!",
                "style": {
                  "fontSize": 20,
                  "fontWeight": "bold",
                  "marginBottom": 10
                }
              }
            },
            {
              "type": "TextInput",
              "props": {
                "placeholder": "Enter your name",
                "style": {
                  "borderWidth": 1,
                  "borderColor": "#ccc",
                  "padding": 10,
                  "marginBottom": 10
                }
              }
            },
            {
              "type": "TouchableOpacity",
              "props": {
                "onPress": "handleClick",
                "style": {
                  "backgroundColor": "#4CAF50",
                  "padding": 12,
                  "borderRadius": 6
                },
                "textStyle": {
                  "color": "#fff",
                  "textAlign": "center",
                  "fontWeight": "bold"
                },
                "children": "Submit"
              }
            }
          ]
        }
      ]
    }
  ]
}
